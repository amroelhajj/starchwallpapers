
@model List<NASA>

<h1>Here is NASA's Image of the Day!</h1>
<ul>
    @foreach (NASA item in Model) {
        <li>
            <h2>@item.Title</h2>
            <a href="@item.Hdimg"><img src="@item.Thumbnail" /></a>
            <p>@item.Tooltip</p>
            <p>Copyright: @item.Copyright</p>
        </li>
    }
</ul>
